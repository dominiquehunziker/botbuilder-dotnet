{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Personname": {
            "type": "string",
            "$entities": [
                "personName"
            ]
        },
        "Age": {
            "type": "object",
            "$entities": [
                "age:Age",
                "age"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Ordinal": {
            "type": "string",
            "$entities": [
                "ordinal"
            ]
        },
        "Geographyv2": {
            "type": "object",
            "$entities": [
                "geographyV2:Geographyv2",
                "geographyV2"
            ],
            "required": [
                "type",
                "location"
            ],
            "properties": {
                "type": {
                    "type": "type"
                },
                "location": {
                    "type": "location"
                }
            }
        },
        "Keyphraseproperty": {
            "type": "string",
            "$entities": [
                "keyPhrase"
            ]
        },
        "Money": {
            "type": "object",
            "$entities": [
                "money:Money",
                "money"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Percentage": {
            "type": "string",
            "$entities": [
                "percentage"
            ]
        },
        "Temperature": {
            "type": "object",
            "$entities": [
                "temperature:Temperature",
                "temperature"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Url": {
            "type": "string",
            "$entities": [
                "url"
            ]
        },
        "Quantity": {
            "type": "number",
            "$entities": [
                "number"
            ],
            "minimum": 1,
            "maximum": 10
        },
        "Utteranceproperty": {
            "type": "string",
            "$entities": [
                "utterance"
            ],
            "$comment": "This indicates the whole string is what maps to this."
        },
        "Dimension": {
            "type": "object",
            "$entities": [
                "dimension:Dimension",
                "dimension"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Enum1": {
            "type": "string",
            "enum": [
                "multiGrainWheat",
                "rye",
                "white",
                "wholeWheat"
            ],
            "$entities": [
                "Enum1Entity"
            ]
        },
        "Enum2": {
            "type": "string",
            "enum": [
                "american",
                "cheddar",
                "feta",
                "gouda",
                "pepper jack",
                "provolone",
                "swiss",
                "none"
            ],
            "$entities": [
                "Enum2Entity"
            ]
        },
        "Zipcodepattern": {
            "type": "string",
            "pattern": "^[0-9]{5}(?:-[0-9]{4})?$",
            "$entities": [
                "utterance",
                "number"
            ]
        },
        "Email": {
            "type": "string",
            "$entities": [
                "email"
            ]
        },
        "CancelConfirmation": {
            "type": "string",
            "$entities": [
                "Confirmation"
            ]
        },
        "ChangePropertyConfirmation": {
            "type": "string",
            "$entities": [
                "Confirmation"
            ]
        },
        "CompleteConfirmation": {
            "type": "string",
            "$entities": [
                "Confirmation",
                "PROPERTYName"
            ]
        },
        "PropertyToChange": {
            "type": "string",
            "enum": "properties",
            "$entities": [
                "PROPERTYName"
            ]
        },
        "PropertyToRemember": {
            "type": "string",
            "enum": "properties",
            "$entities": [
                "PROPERTYName"
            ]
        }
    },
    "required": [
        "Personname",
        "Age",
        "Ordinal",
        "Geographyv2",
        "Keyphraseproperty",
        "Money",
        "Percentage",
        "Temperature",
        "Url",
        "Quantity",
        "Utteranceproperty",
        "Dimension",
        "Enum1",
        "Enum2",
        "Zipcodepattern",
        "Email"
    ],
    "$expectedOnly": [
        "Confirmation",
        "PROPERTYName",
        "utterance",
        "keyPhrase",
        "personName"
    ],
    "$public": [
        "Personname",
        "Age",
        "Ordinal",
        "Geographyv2",
        "Keyphraseproperty",
        "Money",
        "Percentage",
        "Temperature",
        "Url",
        "Quantity",
        "Utteranceproperty",
        "Dimension",
        "Enum1",
        "Enum2",
        "Zipcodepattern",
        "Email"
    ],
    "definitions": {
        "PROPERTYName": {
            "type": "string",
            "enum": "properties",
            "$entities": [
                "PROPERTYName"
            ]
        }
    }
}